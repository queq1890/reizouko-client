  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint ./src",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier -w .",
    "fix:eslint": "yarn lint:eslint --fix",
    "copy-schema": "cp ../reizouko-api/schema.gql schema.graphql",
    "codegen": "npm run copy-schema && apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql --no-addTypename"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.4.0",
    "clsx": "^1.1.1",
    "eslint-plugin-react": "^7.23.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^0.5.6",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "apollo": "^2.33.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1"
  }
}
